generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  image String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model ContentType {
  id    Int     @id @default(autoincrement())
  title String  @unique

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
